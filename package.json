{
  "name": "@bigfootds/sourcepool",
  "productName": "Sourcepool",
  "version": "0.0.1",
  "description": "Sourcepool by BigfootDS",
  "keywords": [],
  "author": {
    "name": "BigfootDS",
    "email": "contact@bigfootds.com",
    "url": "https://bigfootds.com"
  },
  "license": "MIT",
  "private": false,
  "build": {
    "appId": "com.bigfootds.patronsimulator",
    "icon": "./src/assets/logo/sourcepool.ico",
    "includeSubNodeModules": true,
    "buildDependenciesFromSource": true
  },
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-windows": "electron-forge make --target @rabbitholesyndrome/electron-forge-maker-portable",
    "make-ubuntu": "electron-forge make --target @electron-forge/maker-deb",
    "publish": "electron-forge publish",
    "dev:licenseDataGen": "mkdir -p ./out && npx @bigfootds/npm-compliance-helper > ./src/shared/data/compliance.json",
    "dev:react-devtools": "react-devtools"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@bigfootds/npm-compliance-helper": "^1.0.0",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-webpack": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^3.6.0",
    "@rabbitholesyndrome/electron-forge-maker-portable": "^0.2.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.1",
    "electron": "30.0.1",
    "electron-devtools-installer": "^3.2.0",
    "node-loader": "^2.0.0",
    "react-devtools": "^5.1.0",
    "style-loader": "^4.0.0"
  },
  "dependencies": {
    "@bigfootds/supercamo": "^0.4.4",
    "@homebridge/ciao": "^1.2.0",
    "bcrypt": "^5.1.1",
    "bonjour-service": "^1.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "explorer-opener": "^1.0.1",
    "i18next": "^23.11.3",
    "inflection": "^3.0.0",
    "iso-639-1": "^3.1.2",
    "on-change": "^5.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.4",
    "react-i18next": "^14.1.1",
    "react-pure-modal": "^2.2.5",
    "react-router-dom": "^6.23.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^10.0.5",
    "react-use": "^17.5.0",
    "ws": "^8.17.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  }
}
